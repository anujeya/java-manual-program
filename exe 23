public class Main(){  
	  System.out.println("method is invoked");  
	  }  public class Main
{
	  void n(){  
	  this.Main();//no need because compiler does it for you.  
	  }  
	  void p(){  
	  n();//complier will add this to invoke n() method as this.n()  
	  } 
	  public static void main(String args[]){ 
	  
 Main s1 = new Main();  
	  s1.p(); 
}
}
