import java.util.*; 
import java.io.*; 
 public class Main
 {
 public static void main(String[] args)
 {
	int[] a = {11, 15, 13, 20, 45, 10, 33, 53};
    int r = 0; 
	System.out.println("Original Array: "+Arrays.toString(a)); 
	int m = 0;
	int[] n;
	while(a[m] != 10)
	  m++;
	n = new int[m];
	for(int i = 0; i < m; i++)
		n[i] = a[i];
	System.out.println("New Array: "+Arrays.toString(n)); 
  }
}
